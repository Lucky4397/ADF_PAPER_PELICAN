{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "PAPER-PALICAN-ADF"
		},
		"PAPER_PELICAN_BLOB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'PAPER_PELICAN_BLOB'"
		},
		"NASA_ASTERIOD_NEOBS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.nasa.gov/neo/rest/v1/feed"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "PAPER-PALICAN-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"PAPER_PELICAN_BLOB_connectionString": {
						"value": "[parameters('PAPER_PELICAN_BLOB_connectionString')]"
					},
					"NASA_ASTERIOD_NEOBS_properties_typeProperties_url": {
						"value": "[parameters('NASA_ASTERIOD_NEOBS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}